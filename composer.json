{
    "name": "apimediaru/nova-tinymce",
    "description": "A Laravel Nova field that provides integration of TinyMCE.",
    "keywords": [
        "apimediaru",
        "tinymce",
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "APIMedia\\TinyMCE\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "APIMedia\\TinyMCE\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
